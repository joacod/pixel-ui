{
  "name": "pixel-ui-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for pixel-ui component library and documentation",
  "scripts": {
    "dev": "pnpm --filter www dev",
    "build": "pnpm --recursive build",
    "build:lib": "pnpm --filter @joacod/pixel-ui build",
    "build:docs": "pnpm --filter www build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build:lib && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7"
  },
  "packageManager": "pnpm@9.0.0"
}
